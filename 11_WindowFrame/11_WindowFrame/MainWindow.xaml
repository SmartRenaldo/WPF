<Window x:Class="_11_WindowFrame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_11_WindowFrame"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame Name="myFrame" VerticalAlignment="Top" 
               HorizontalAlignment="Left" Width="800" Height="200" Margin="53,127,0,0"
               NavigationUIVisibility="Visible" Source="..\Pages\MyPage.xaml" LoadCompleted="myFrame_LoadCompleted"/>
        <Button Content="Go to Google" HorizontalAlignment="Left" Margin="500,230,0,0" 
                VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
