<Window x:Class="_33_Rectangle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_33_Rectangle"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Rectangle Stroke="Black" HorizontalAlignment="Center" VerticalAlignment="Center" Height="300" 
                   Width="300" StrokeThickness="10" StrokeDashArray="1" RadiusX="100" RadiusY="100">
            <Rectangle.Fill>
                <!--<VisualBrush>
                    <VisualBrush.Visual>
                        <Image Source="../kitty.jpg" Stretch="Uniform"/>
                    </VisualBrush.Visual>
                </VisualBrush>-->
                <DrawingBrush>
                    <DrawingBrush.Drawing>
                        <ImageDrawing ImageSource="../kitty.jpg"></ImageDrawing>
                    </DrawingBrush.Drawing>
                </DrawingBrush>
            </Rectangle.Fill>
        </Rectangle>
    </Grid>
</Window>
